{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "barryvdh/laravel-dompdf": "^0.8.5",
        "barryvdh/laravel-snappy": "^0.4.6",
        "browner12/helpers": "^3.3",
        "endroid/qr-code": "^4.0",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.3@dev",
        "h4cc/wkhtmltoimage-amd64": "0.12.x",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "intervention/image": "^2.5.1",
        "khanamiryan/qrcode-detector-decoder": "^1.0",
        "knplabs/knp-snappy": "^1.2.1",
        "laravel/framework": "^8.75",
        "laravel/installer": "^4.2",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "laravelcollective/html": "^6.2.1",
        "league/flysystem-aws-s3-v3": "~1.0",
        "league/flysystem-cached-adapter": "~1.0",
        "maatwebsite/excel": "^3.1",
        "malc0mn/vultr-api-client": "^1.1",
        "orangehill/iseed": "^3.0",
        "pda/pheanstalk": "^4.0.0",
        "predis/predis": "^1.1.1",
        "pusher/pusher-php-server": "~4.0",
        "shivella/laravel-bitly": "^1.1",
        "watson/rememberable": "^5.0.1",
        "yugo/moota": "^1.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "brianium/paratest": "^6.5",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.19",
        "kitloong/laravel-migrations-generator": "^6.3",
        "laravel-shift/factory-generator": "dev-master",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10",
        "thedoctor0/laravel-factory-generator": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
